{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.1.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "enqueue/async-event-dispatcher": "^0.9.0@dev",
    "enqueue/enqueue-bundle": "^0.9@dev",
    "enqueue/messenger-adapter": "*@dev",
    "enqueue/rdkafka": "^0.9@dev",
    "monolog/monolog": "^2.0@dev",
    "symfony/console": "*",
    "symfony/flex": "^1.1",
    "symfony/framework-bundle": "*",
    "symfony/serializer": "4.2.*",
    "symfony/yaml": "*"
  },
  "require-dev": {
    "symfony/dotenv": "*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Mercur\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mercur\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "4.2.*"
    }
  }
}
